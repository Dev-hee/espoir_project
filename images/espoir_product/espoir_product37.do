	function ga4TaggingTrigger(evtType, data){
		
		let pushFlag = true;
		
		
		if(evtType == 'login'){
			
			window.dataLayer.push({
			    'event':'login_complete'
			    , 'event_category':'login'
			    , 'event_action':'login - complete'
			    , 'event_label': data
			});
			
			pushFlag = false;
			
		
		} else if(evtType == 'sign_up'){
			
			window.dataLayer.push({
			    'event':'sign_up_complete'
			    , 'event_category':'sign_up'
			    , 'event_action':'sign_up - complete'
			    , 'event_label': data
			});
			
			pushFlag = false;
			
		
		} else if(evtType == 'withdrawal'){
			
			window.dataLayer.push({
			    'event':'withdrawal_complete'
			    , 'event_category':'withdrawal'
			    , 'event_action':'withdrawal - complete'
			    , 'event_label': data
			});
			
			pushFlag = false;
			
		
		} else if(evtType == 'review'){
			
			AP_REVIEW_PRD									= data.i_sProductnm;
			AP_REVIEW_PRDCODE								= data.i_sProductcd;
			AP_REVIEW_RATING								= data.i_iUsePoint;
			AP_REVIEW_PICTURE								= data.REVIEW_thumbnail_id != null ? data.REVIEW_thumbnail_id.length : 0;
			
			AP_REVIEW_CONTENT								= data.i_sContent;
			
		
		} else if(evtType == 'promotion_action'){
			
			AP_PROMO_ACTION									= data.action_name;
			
		
		} else if(evtType == 'select_item'){
			
			window.dataLayer.push({
			  'event': evtType,
			  'prdInfo': {
			    'code': data.code,
			    'name': data.name,
			    'brand': data.brand,
			    'cate': data.cate,
			    'apg_brand_code': data.apg_brand_code,
			    'index': data.index,
			    'item_list_name': data.item_list_name,
			    'price': data.price,
			    'discount': data.discount,
			    'prdprice': data.prdprice
			  }
			});
			
			pushFlag = false;
			
		
		} else if(evtType == 'cart') {
			
			AP_CART_PRICE = data.ap_cart_price;
			AP_CART_PRDPRICE = data.ap_cart_prdprice;
			AP_CART_DISCOUNT = data.ap_cart_discount;
			AP_CART_PRDS = data.ap_cart_prds;
			
		
		} else if(evtType == 'addcart') {
			
			var jsonArr = new Array();
			for(var i = 0 ; i < data.length; i++){
				var temp = new Object();
				temp.code = data[i].v_productcd;
				temp.name = data[i].v_productnm; 
				temp.brand = "에스쁘아";
				temp.cate = data[i].v_ctg_nm || undefined;
				temp.apg_brand_code = (data[i].v_optioncd).substr(0, 5);
				temp.quantity = data[i].n_cnt;
				temp.price = data[i].n_price;
				temp.discount = data[i].v_member_dis
				temp.prdprice = Number(data[i].n_list_price);
				temp.variant = data[i].v_optionnm;
				
				
				
				
				jsonArr.push(temp);
			}
			// AP_CART_ADDPRDS = JSON.parse(JSON.stringify(jsonArr));
