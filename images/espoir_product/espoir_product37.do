		} else if(evtType == 'purchaseprdbtn') {
			
			for(var i = 0 ; i < data.length; i++){
				window.dataLayer.push({'event': evtType, 'quantity': data.n_cnt});
			}
			
			pushFlag = false;
			
		}

		if(pushFlag) {
			window.dataLayer.push({'event': evtType});
		}
		
		console.log("ga4TaggingTrigger dataLayer.push({'event' : " + evtType + (pushFlag ? "" : ", ... ") + "})");
		
